{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "chuck-norris-route": {
        "ClusterId": "chuck-norris-cluster",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "/jokes/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "chuck-norris-cluster": {
        "Destinations": {
          "chuck-norris-destination": {
            "Address": "https://api.chucknorris.io"
          }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=54325;Database=accounts;Username=postgres;Password=password"
  },
  "JwtSettings": {
    "SecretKey": "key"
  }
}
