{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "songs": {
        "ClusterId": "songs",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "songs/{**catch-all}"
        }
      },
      "accounts": {
        "ClusterId": "accounts",
        "AuthorizationPolicy": "default",
        "Match": {
          "Path": "accounts/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "songs": {
        "Destinations": {
          "destination": {
            "Address": "http://host.docker.internal:8080/"
          }
        }
      },
       "accounts": {
        "Destinations": {
          "destination": {
            "Address": "http://host.docker.internal:5001/"
          }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "Auth0": {
    "Domain": "",
    "Audience": ""
  }
}